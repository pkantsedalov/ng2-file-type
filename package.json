{
  "name": "ng2-file-type",
  "version": "0.0.1",
  "license": "MIT",
  "description": "Angular input file type directive",
  "keywords": [
    "angular2",
    "angularjs",
    "ng",
    "ng2",
    "input",
    "file",
    "type",
    "mime",
    "directive",
    "validation",
    "angular-file-type",
    "angular-file-validation"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/pkantsedalov/ng2-file-type.git"
  },
  "bugs": {
    "url": "https://github.com/pkantsedalov/ng2-file-type/issues"
  },
  "homepage": "https://github.com/pkantsedalov/ng2-file-type#readme",
  "scripts": {
    "lite-server": "lite-server -c demo/bs-config.json",
    "demo.serve": "run-s build link demo.build lite-server",
    "demo.build": "ng build -prod --aot --bh /",
    "demo.watch": "ng build -prod --aot --bh / --watch",
    "demo.build.gh-pages": "ng build -prod --aot --bh /ng2-file-type/ && cp ./demo/dist/index.html ./demo/dist/404.html",
    "demo.deploy": "gh-pages -d demo/dist",
    "demo.gh-pages": "run-s build demo.build.gh-pages demo.deploy",
    "build": "ngm build -p ./src --clean",
    "build.watch": "ngm build -p ./src --watch --skip-bundles",
    "flow.changelog": "conventional-changelog -i CHANGELOG.md -s -p angular",
    "flow.github-release": "conventional-github-releaser -p angular",
    "start": "ng serve --aot",
    "link": "ngm link -p src --here",
    "lint": "exit 0",
    "pretest": "run-s lint build link",
    "publish": "ngm publish -p ./dist",
    "publish.test": "ngm publish -p ./dist --skip-publish",
    "test": "ng test -sr",
    "test-coverage": "ng test -sr -cc",
    "e2e": "ng e2e --bh /",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "version": "npm run flow.changelog && git add -A"
  },
  "main": "bundles/ng2-file-type.umd.js",
  "module": [
    {
      "name": "ng2-file-type",
      "root": "src",
      "outDir": "dist",
      "main": "index.ts",
      "tsconfig": "tsconfig.json"
    }
  ],
  "peerDependencies": {
    "@angular/core": "^4.0.2",
    "@angular/forms": "^4.0.2"
  },
  "dependencies": {},
  "devDependencies": {
    "@angular/cli": "^1.0.0",
    "@angular/common": "^4.0.2",
    "@angular/compiler": "^4.0.2",
    "@angular/compiler-cli": "^4.0.2",
    "@angular/core": "^4.0.2",
    "@angular/forms": "^4.0.2",
    "@angular/http": "^4.0.2",
    "@angular/platform-browser": "^4.0.2",
    "@angular/platform-browser-dynamic": "^4.0.2",
    "@angular/router": "^4.0.2",
    "@types/jasmine": "2.5.38",
    "@types/node": "^6.0.70",
    "@types/webpack": "^2.2.15",
    "array-includes": "^3.0.3",
    "chromedriver": "^2.29.0",
    "classlist-polyfill": "^1.0.3",
    "codelyzer": "~2.0.0-beta.1",
    "conventional-changelog-cli": "^1.3.1",
    "conventional-github-releaser": "^1.1.3",
    "core-js": "^2.4.1",
    "gh-pages": "^0.12.0",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "lite-server": "^2.3.0",
    "ngm-cli": "^0.5.0",
    "npm-run-all": "^4.0.2",
    "protractor": "~4.0.13",
    "rxjs": "^5.2.0",
    "ts-node": "1.2.1",
    "tslib": "^1.6.1",
    "tslint": "^4.3.0",
    "tslint-config-valorsoft": "^1.2.0",
    "typescript": "^2.2.2",
    "zone.js": "^0.8.5"
  }
}
